import wollok.game.*
import auto.* /** importa el objeto auto del archivo auto.wlk */

program miPrimerWollokGame {
	// limites dele tablero de juego
	game.width(14)
	game.height(8)
	
	// fondo del tablero de juego
	game.boardGround("empedrado.jpg")
	
	/** agregamos a nuestro objeto auto */
	game.addVisual(auto)
	
	// que arranque el juego!
	game.start()
}